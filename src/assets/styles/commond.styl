.cmm-content {
	.search-bar {
		padding-top 10px;
	}
	a {
		color #20a0ff;
	}
	a.small {
		font-size 12px;
	}
	a:hover, a:focus {
		color #4f88bb;
		text-decoration none;
	}
	.dialog-loading {
		.el-dialog__body {
			padding-bottom 10px;
		}
		.dialog-loading-body {
			.dialog-footer{
				text-align right;
				margin-top 20px;
			}
		}
	}
	.content {
		margin-top 0px;
		padding 30px;
	}
	.search-bar {
		border 0;
	}
	.exprot-data {
		text-align right;
		border solid 1px #e0e1e1;
		margin 10px;
		padding 10px;
		border-radius 5px;
	}
	.el-loading-mask {
		background-color rgba(251, 253, 255, 0.6);
	}
	.upload-pic {
		img {
			max-height 150px;
			max-width 200px;
		}
	}
	.remark {
		font-size 12px;
		background #ccc;
		padding 2px 5px;
		border-radius 5px;
		color #fff;
		font-weight bold;
	}
	.remark-stat {
		font-size 12px;
		color #666;
		margin-bottom 5px;
	}
	.tb-item-img {
		max-width 60px;
		max-height 60px;
	}
	.container-num {
		.el-input {
			width 180px;
		}
	}
}